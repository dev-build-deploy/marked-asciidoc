  "license": "MIT",
  "keywords": [
    "Marked",
    "Markdown",
    "Asciidoc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-build-deploy/marked-asciidoc.git"
  },
  "bugs": {
    "url": "https://github.com/dev-build-deploy/marked-asciidoc/issues"
  },
  "homepage": "https://github.com/dev-build-deploy/marked-asciidoc#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "marked": "^8.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/*"
  ]
